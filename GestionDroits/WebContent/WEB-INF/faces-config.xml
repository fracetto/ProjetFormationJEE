<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">
<application>
<!-- DEBUG SPRING -->
<el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>

<locale-config>
<default-locale>fr</default-locale>
<supported-locale>en</supported-locale>
</locale-config>


</application>
<navigation-rule>
<!-- point de depart  -->
<from-view-id>*</from-view-id>
<navigation-case>
<from-outcome>index</from-outcome>
<to-view-id>/index.xhtml</to-view-id>
</navigation-case>
<navigation-case>
			<from-outcome>user</from-outcome>
			<to-view-id>/user.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>ajouterProfil</from-outcome>
			<to-view-id>/ajouterProfil.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>updateProfil</from-outcome>
			<to-view-id>/updateProfil.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>afficherUser</from-outcome>
			<to-view-id>/afficherUser.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>listeProfils</from-outcome>
<!-- 			<to-view-id>/listeProfils_static.xhtml</to-view-id> -->
<to-view-id>/listeProfils.xhtml</to-view-id>
		</navigation-case>
</navigation-rule>
</faces-config>
